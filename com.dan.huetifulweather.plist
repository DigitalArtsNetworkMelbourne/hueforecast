<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>EnvironmentVariables</key>
	<dict>
		<key>HUE_BRIDGE_IP</key>
		<string>{YOUR_HUE_BRIDE_IP}</string>
		<key>HUE_USERNAME</key>
		<string>{YOUR_HUE_USERNAME}string>
	</dict>
	<key>Label</key>
	<string>com.dan.huetifulweather</string>
	<key>ProgramArguments</key>
	<array>
		<string>/bin/bash</string>
		<string>-c</string>
		<string>/{PYTHONPATH}/python /{WORKING_DIRECTORY}/huetifulweather.py</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>StartInterval</key>
	<integer>300</integer>
</dict>
</plist>
